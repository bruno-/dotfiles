# routes incoming mail to appropriate mailboxes
PATH=/usr/bin:/usr/local/bin
MAILDIR=$HOME/data/mail/bruno-lockstate.com/
DEFAULT=$HOME/data/mail/bruno-lockstate.com/inbox/
LOGFILE=/dev/null
SHELL=/bin/sh

:0fw
| $HOME/data/bin/mail-date-header

:0:
* ^X-getmail-retrieved-from-mailbox: \[Gmail\]/Sent Mail
archive/

:0:
* ^X-getmail-retrieved-from-mailbox: \[Gmail\]/All Mail
/dev/null

:0:
* ^X-getmail-retrieved-from-mailbox: Mail Archive
archive/

:0:
* ^Subject: Confirmation instructions
archive/

:0:
* ^From:.*noreply@aylanetworks.com
archive/

:0:
* ^From:.*automated@airbnb.com
archive/

:0:
* ^From:.*support@eleviomail.com
archive/
